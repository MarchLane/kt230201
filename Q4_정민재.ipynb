    "# Q4 Answer Template\n",
    "data = input('숫자로 이루어진 문자열을 입력하세요. ')\n",
    "       \n",
    "data = int(data)\n",
    "data = str(data)\n",
    "\n",
    "result = 1\n",
    "\n",
    "for a in range(0, len(data)) :\n",
    "    if int(data[a]) == 0 :\n",
    "        result = result + int(data[a])\n",
    "    else :\n",
    "        result = result * int(data[a])\n",
    "\n",
    "print(result)"
